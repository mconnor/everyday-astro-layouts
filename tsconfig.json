{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "/Users/mikeconnor/code/code-2024/css-layouts/every-layout-1",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@cc/*": [
        "src/customComponents/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@content/*": [
        "src/content/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@scripts/*": [
        "src/scripts/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": [
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.ts",
    "src/**/*.astro"
  ],
  "exclude": [
    "node_modules"
  ]
}